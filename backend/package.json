{
    "name": "n8n-mcp-backend",
    "version": "1.0.0",
    "type": "module",
    "description": "Backend API for n8n MCP Platform",
    "main": "dist/index.js",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "postinstall": "prisma generate",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:studio": "prisma studio",
        "deploy": "prisma db push --accept-data-loss && npm run build",
        "test": "vitest",
        "test:run": "vitest run"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.32.1",
        "@fastify/cors": "^10.0.2",
        "@fastify/helmet": "^13.0.1",
        "@fastify/jwt": "^9.0.2",
        "@fastify/rate-limit": "^10.2.2",
        "@prisma/client": "^6.1.0",
        "@supabase/supabase-js": "^2.47.12",
        "@types/bcryptjs": "^2.4.6",
        "@types/node": "^22.10.2",
        "axios": "^1.7.9",
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.4.7",
        "fastify": "^5.2.0",
        "prisma": "^6.1.0",
        "typescript": "^5.7.2",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "tsx": "^4.19.2",
        "vitest": "^2.1.8"
    }
}